@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>User info</p>

<table class="table table-striped">
    <thead>
    <tr>
        <th>Claim name</th>
        <th>Claim value</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var claim in User.Claims)
    {
        <tr>
            <td>
                @claim.Type
            </td>
            <td>
                @claim.Value
            </td>
        </tr>
    }
    </tbody>
</table>

